{
  "globalName": "gslb-operator.io",
  "balanceType": "wrr",
  "type" : "http",
  "interval": 2,
  "monitor": {
    "https" : true,
    "host" : "dummyjson.com",
    "path" : "/test",
    "headers" : {
      "User-Agent" : "curl"
    },
    "port": 443,
    "valid_codes": [200, 201],
    "timeout": 2,
    "retries": 2
  },
  "tcp_monitor": {
    "interval": 2,
    "retries": 2,
    "port": 80
  },
  "http_monitor" : {
    "https" : true,
    "headers" : {
      "host" : "example.com",
      "x-api-token" : "qwerty"
    },
    "port": 80,
    "valid_codes": [200, 201],
    "timeout": 2,
    "retries": 2
  },
  "members": {
    "ip2" : {
      "ip": "104.196.232.237",
      "weight": 1
    }
  }
}